{
  "name": "mysql-source-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "topic.prefix": "mysqlserver1",

    "database.hostname": "${envVarProvider:MYSQL_HOST}",
    "database.port": "${envVarProvider:MYSQL_PORT}",
    "database.user": "${envVarProvider:MYSQL_USER}",
    "database.password": "${envVarProvider:MYSQL_PASSWORD}",

    "database.server.id": "5400",
    "database.server.name": "cdc-sdb",
    "database.include.list": "customers",

    "schema.history.internal.kafka.bootstrap.servers": "kafka-1:19092,kafka-2:19092,kafka-3:19092",
    "schema.history.internal.kafka.topic": "schema-changes.testdb",

    "heartbeat.interval.ms": 30000,
    "include.schema.changes": "true"
  }
}

